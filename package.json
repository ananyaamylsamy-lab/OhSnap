{
  "name": "ohsnap",
  "version": "1.0.0",
  "description": "Photography location and shot tracking platform",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "cd backend && node server.js",
    "dev": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm install && npm run build && cd ../backend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "install-backend": "cd backend && npm install",
    "install-frontend": "cd frontend && npm install",
    "install-all": "npm run install-backend && npm run install-frontend",
    "lint": "eslint .",
"lint:fix": "eslint . --fix",
"format": "prettier --write frontend backend",
"fix:all": "npm run format && npm run lint:fix"
  },
  "author": "Manasha Sekar & Ananyaa Mylsamy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ananyaamylsamy-lab/OhSnap.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^16.5.0",
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "mapbox-gl": "^3.16.0"
  }
}
